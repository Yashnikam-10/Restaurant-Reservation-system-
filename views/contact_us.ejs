<%- include('partials/header') %>

  <div class="container container_2">
    <div class="content">
      <div class="left-side">
        <div class="address details">
          <i class="fas fa-map-marker-alt"></i>
          <div class="topic">Address</div>
          <div class="text-one">Mahatma Gandhi Marg</div>
          <div class="text-two">Thane, Mumbai</div>
        </div>
        <div class="phone details">
          <i class="fas fa-phone-alt"></i>
          <div class="topic">Phone</div>
          <div class="text-one">+91 809 078 4675</div>
          <div class="text-two">+91 809 078 4685</div>
        </div>
        <div class="email details">
          <i class="fas fa-envelope"></i>
          <div class="topic">Email</div>
          <div class="text-one">resdine@gmail.com</div>
          <div class="text-two">info.resdine@gmail.com</div>
        </div>
